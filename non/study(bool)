//Polymorphism
//inheritance
//Encapsulation
//ABSTRACTION : Take what you want of the problem and ignore everything else only implement that one
#include <iostream>
using namespace std;

int main() {
bool yujin = true;
int a =25;
    cout << "yujin : " << yujin << endl;
    cout << "yujin2 : " << !yujin << endl;
    yujin=2345; //2345 is not zero therefore yujin = 1

    cout << "mdmdm" << yujin << endl;
    cout << !!a << endl; //25는 0 이아니니까 !a는 0, !!a은 1
    return 0;
}
